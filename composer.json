{
    "name": "orchidcommunity/backup",
    "description": "",
    "keywords": [
        "orchid",
        "backup"
    ],
    "homepage": "https://github.com/orchid/builder",
    "license": "MIT License (MIT)",
    "type": "library",
    "authors": [
        {
            "name": "Alexandr Chernyaev",
            "email": "bliz48rus@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "spatie/laravel-backup": "^6.0",
        "orchid/platform": "^3.8"
    },
    "require-dev": {
        "orchestra/testbench": "3.7.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Orchid\\Backup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchid\\Backup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orchid\\Platform\\Providers\\FoundationServiceProvider",
                "Orchid\\Backup\\BackupServiceProvider"
            ]
        }
    }
}
